n := Length(Gop);
// C := RandomLinearCode(Fq,n,r*m);

s := Floor(f_hat)-4;
print "Shortening ", s , " times\n";
k := r*m-s;
G_Fqm := ChangeRing(GeneratorMatrix(Gop),Fqm);
Gop_Fqm := LinearCode(G_Fqm);
// Find a Moore basis
Moore := ZeroMatrix(Fqm,r*m,n);
while Rank(Moore) lt r*m do 
    elts := [Random(Gop_Fqm) : _ in [1..r]];
    for j in [1..m] do
        for i in [1..r] do 
            for k in [1..n] do 
                Moore[(j-1)*r+i,k] := elts[i][k]^(2^j);
            end for;
        end for;
    end for;
end while;


P := Solution(Moore,G_Fqm);

S := RotateRows(IdentityMatrix(Fqm,r*m),1,r*m,r);

function ShiftAndConjugate(M)
    Res := ZeroMatrix(BaseRing(M),r*m,r*m);
    for i in [1..r*m] do 
        for j in [1..r*m] do 
            Res[i,j] := M[i,j]^q;
        end for;
    end for;
    return Transpose(S) * Res * S;
end function;


Rank2MatricesGoodBasis := [];
Rank2MatricesBadBasis := [];

e := Ceiling(Log(2,r))+1;

Nmat := Floor(e * r / 3) + 2;
t := 0;
Group := Sym(r*m);
while true do
    if t gt Nmat then
        break;
    end if;
    sigma := Random(Group);
    RemovedIndices := [i^sigma : i in [1..s]];

    G := Submatrix(GeneratorMatrix(Gop),[i : i in [1..r*m] | not i in RemovedIndices],[j : j in [1..n] | not j in RemovedIndices]);

    G2 := Matrix([[G[tup[1],l]*G[tup[2],l] : l in [1..n-s]] : tup in CartesianPower([1..k],2) | tup[1] le tup[2]]);

    K := Basis(Kernel(G2));

    Cmat := [MatrixOfPolarForm(rel,k) : rel in K];
    N := #Cmat;

    print "N = ", N;

    // Specialize variables
    spec := 1;

    R := PolynomialRing(Fq,N-spec,"grevlex");
    AssignNames(~R,["x"*IntegerToString(i) : i in [spec+1..N]]);

    SetVerbose("Faugere",2);

    Mx := &+[R.i * Cmat[i] : i in [1..N-spec]] + Cmat[N];

    print "Computing Pfaffians...";
    Pfs := Pfaffians(Mx,4);
    I := Ideal(Pfs);
    print "Done.\nNow computing Dimension...";

    print "Dimension(I) = ", Dimension(I);

    V := Variety(I,Fqm);
    DmatBadBasis := [ExtendSquareMatrix(Evaluate(Mx,v),r*m,RemovedIndices) : v in V];
    DmatGoodBasis := [Transpose(P) * M * P : M in DmatBadBasis];

    Append(~Rank2MatricesGoodBasis,DmatGoodBasis);
    Append(~Rank2MatricesBadBasis,DmatBadBasis);


    print "#V = ", #V;
    print "m = ", m;

    M := DmatGoodBasis[1];
    for _ in [1..m] do 
        M := ShiftAndConjugate(M);
        M in DmatGoodBasis;
    end for;
    t := t + 1;
end while;

function Distinguish(A,B)
    u1 := Basis(Image(A))[1];
    v1 := Basis(Image(A))[2];

    u2 := Basis(Image(B))[1];
    v2 := Basis(Image(B))[2];

    c1 := u1 * Moore;
    d1 := v1 * Moore;
    h1 := Vector([hi^(2^(m-1)) : hi in Eltseq(c1*d1)]);

    c2 := u2 * Moore;
    d2 := v2 * Moore;
    h2 := Vector([hi^(2^(m-1)) : hi in Eltseq(c2*d2)]);

    return Dimension(SquareCode(LinearCode(Matrix([c1,d1,h1,c2,d2,h2]))));
end function;

function Align(A,B)
    while Distinguish(A,B) gt 15 do 
        B := ShiftAndConjugate(B);
    end while;
    return B;
end function;


AlignedMatrices := [Rank2MatricesGoodBasis[1][1]];
for j in [2..#Rank2MatricesGoodBasis] do
    B := Rank2MatricesGoodBasis[j][1];
    B := Align(AlignedMatrices[1],B);
    Append(~AlignedMatrices,B);
end for;

Code := [];
for A in AlignedMatrices do 
    u := Basis(Image(A))[1] * Moore;
    v := Basis(Image(A))[2] * Moore;
    w := Vector([wi^(2^(m-1)) : wi in Eltseq(u*v)]);
    Append(~Code,[u,v,w]);
end for;

Code := &cat(Code);

BV := GeneratorMatrix(LinearCode(Matrix(Code)));
for _ in [1..e-1] do 
    BVq := Matrix([[BV[i,j]^(q^(m-1)) : j in [1..n]] : i in [1..Nrows(BV)]]);
    BV := BasisMatrix(Rowspace(BV) meet Rowspace(BVq));
end for;

GRS := LinearCode(BV);
C := LinearCode(BV);
for _ in [1..r-2] do
    C := IntersectCode(C, ProductCode(C,GRS));
end for;

// Now C is GRS_2
D := IntersectCode(C, ProductCode(C,GRS));
RS2 := ConductorCode(D,C);

y := Rows(GeneratorMatrix(D))[1];
while true do 
    x := Random(RS2);
    if not &and([x[i] eq x[i+1] : i in [1..n-1]]) then 
        break;
    end if;
end while;

// Check the attack
Test := Dual(AlternantCode(Eltseq(x),Eltseq(y),r,Fq));
check1 := Test eq Gop;

check2 := Degree(Interpolation(Eltseq(x),[1 / yi : yi in Eltseq(y)])) eq r;
print("[ATTACK SUCCESS]: ");
print(check1 and check2);


