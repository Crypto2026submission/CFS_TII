function SquareCode(C)
    F := BaseField(C);
    G := GeneratorMatrix(C);
    n := Length(C);
    k := Dimension(C);
    G2 := Matrix([[G[tup[1],l]*G[tup[2],l] : l in [1..n]] : tup in CartesianPower([1..k],2) | tup[1] le tup[2]]);
    return LinearCode(G2);
end function;

function ProductCode(C,D)
    G1 := GeneratorMatrix(C);
    G2 := GeneratorMatrix(D);
    assert Ncols(G1) eq Ncols(G2);
    n := Ncols(G1);
    k1 := Nrows(G1);
    k2 := Nrows(G2);
    return LinearCode(Matrix([[G1[tup[1],l]*G2[tup[2],l] : l in [1..n]] : tup in CartesianProduct([1..k1],[1..k2])]));
end function;

function ConductorCode(C,D)
    return Dual(ProductCode(C,Dual(D)));
end function;

function IntersectCode(C,D)
    return LinearCode(BasisMatrix(Rowspace(GeneratorMatrix(C)) meet Rowspace(GeneratorMatrix(D))));
end function;

function QuadraticHull(G)
    n := Ncols(G);
    k := Nrows(G);
    G2 := Matrix([[G[tup[1],l]*G[tup[2],l] : l in [1..n]] : tup in CartesianPower([1..k],2) | tup[1] le tup[2]]);
    return Basis(Kernel(G2));
end function;

function MatrixOfPolarForm(rel,k)
    M := ZeroMatrix(BaseRing(rel),k,k);
    count := 1;
    for i in [1..k] do
        for j in [i..k] do
            if i eq j then
                M[i,i] := 2*rel[count];
            else 
                M[i,j] := rel[count];
                M[j,i] := rel[count];
            end if;
            count := count + 1;
        end for;
    end for;
    return M;
end function;

function ExtendSquareMatrix(Mat,M,RemovedIndices)
    Res := ZeroMatrix(BaseRing(Mat),M,M);
    Indices := [i : i in [1..M] | not i in RemovedIndices];
    NI := #Indices;
    assert NI eq Ncols(Mat);
    for i in [1..NI] do 
        for j in [1..NI] do 
            Res[Indices[i],Indices[j]] := Mat[i,j];
        end for;
    end for;
    return Res;
end function;


